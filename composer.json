{
    "name": "chllen/hyperf-service-micro-etcd",
    "description": "A etcd adapter for service go-micro.",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "service",
        "etcd-adapter"
    ],
    "require": {
        "php": ">=7.2",
        "hyperf/contract": "~2.2.0",
        "hyperf/service-governance": "~2.2.0",
        "hyperf/utils": "~2.2.0",
        "chllen/hyperf-grpc-client": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Chllen\\HyperfServiceMicroEtcd\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Chllen\\HyperfServiceMicroEtcd\\ConfigProvider"
        }
    }
}
